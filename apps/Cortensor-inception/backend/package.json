{
  "name": "inception-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run --reporter verbose",
    "e2e:env-check": "ts-node scripts/env-check.ts",
    "e2e:submit": "ts-node scripts/submit-task.ts",
    "e2e:listen": "ts-node scripts/listen-claims.ts",
    "e2e:claims-to-decision": "ts-node scripts/claims-to-decision.ts",
    "e2e:decision-to-evidence": "ts-node scripts/decision-to-evidence.ts",
    "e2e:audit": "ts-node scripts/audit.ts",
    "e2e:gen-attestor": "ts-node scripts/gen-attestor-key.ts",
    "e2e:print-signer": "ts-node scripts/print-signer.ts",
    "e2e:verify-abi": "ts-node scripts/verify-abi.ts",
    "e2e:full": "ts-node scripts/full-pipeline.ts",
    "e2e:sim-variant": "ts-node scripts/simulate-variant.ts",
    "e2e:llm": "ts-node scripts/llm-claims.ts",
    "e2e:blend": "ts-node scripts/blend-results.ts",
    "e2e:full-blend": "bash -lc \"npm run e2e:sim-variant -- unanimous 200 attestor.key out-variants && LLM_MODEL='qwen2.5:0.5b' npm run e2e:llm -- \"https://ollama-qwen.zeabur.app/api/chat\" \"Is Event X likely to have occurred?\" 3 0.7 attestor.key out-llm && npm run e2e:blend\""
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.2",
    "adk-ts": "^0.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/node": "^20.7.2",
    "@types/supertest": "^2.0.12",
    "ethers": "^5.7.2",
    "prisma": "^5.15.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.1.5"
  }
}
